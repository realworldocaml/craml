(alias
 ((name runtest)
  (deps (comments.sh comments.sh.expected))
  (action (progn
           (run ${exe:../bin/main.exe} ${<})
           (diff? ${<}.expected ${<}.corrected)))))

(alias
 ((name runtest)
  (deps (lines.sh))
  (action (progn
           (run ${exe:../bin/main.exe} ${<})
           (diff? ${<} ${<}.corrected)))))

(alias
 ((name runtest)
  (deps (exit.sh exit.sh.expected))
  (action (progn
           (run ${exe:../bin/main.exe} ${<})
           (diff? ${<}.expected ${<}.corrected)))))
